<html>
<head>
<title>Full details</title></head>
<style>
.btn {
  background-color: #4CAF50;
  color: white;
  padding: 12px;
  margin: 10px 0;
  border: none;
  width: 20%;
  border-radius: 3px;
  cursor: pointer;
  font-size: 17px;
}
h1 {
    font-family: Georgia, serif;
    border-bottom: 3px solid #cc9900;
    color: #996600;
    font-size: 30px;
}
body {

    font-family: "Trebuchet MS", Verdana, sans-serif;
    font-size: 16px;
    color: #696969;
    padding: 3px;
}
#main {
    padding: 5px;
    padding-left:  15px;
    padding-right: 15px;
    background-color: #ffffff;
    border-radius: 0 0 5px 5px;
}
</style>
<h1>VEHICLE FULL DETAILS</h1>
<body>
<div id="main">
<h2>List of Vehicle Detailed Information</h2>
  <ul class="vehicles">
  <% for(var i=0; i<vehicles.length; i++) {%>
    <li class="vehicles">
      <span><%= " Full Name: " +vehicles[i].fname%><br></span>
      <span><%=" Email: " + vehicles[i].email%><br></span>
	   <span><%= " Aadhar Number: " +vehicles[i].aadhar%><br></span>
      <span><%=" Address: " + vehicles[i].adr%><br></span>
	   <span><%= " Area: " +vehicles[i].are%><br></span>
      <span><%=" City: " + vehicles[i].city%><br></span>
	   <span><%= " Pincode: " +vehicles[i].pin%><br></span>
      <span><%=" Contact Number: " + vehicles[i].mobile%><br></span>
	   <span><%= " Alternate Number: " +vehicles[i].alt%><br></span>
      <span><%=" Vehicle Type: " + vehicles[i].vtype%><br></span>
	   <span><%= " Company Name: " +vehicles[i].cname%><br></span>
      <span><%=" Vehicle Name: " + vehicles[i].vname%><br></span>
	   <span><%= " Vehicle Registration Number: " +vehicles[i].rgno%><br></span>
      <span><%=" Model Name: " + vehicles[i].mname%><br></span>
	  <span><%= " Model Number: " +vehicles[i].mno%><br></span>
     <br><br>
    </li>
  <% } %>
</ul>
</div>
</body>
 <div class="col-50">   
  <form action="/index.html" >
    <button class="btn" >Home</button>
  </form>  
     </div>
	 </html>